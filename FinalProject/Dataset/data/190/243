<!-- wAZxU9 --><SCRIPT language=javascript>
<!-- jly4Wh --><!--
	function fPopUp(tURL, nWidth, nHeight){
		var xMax, yMax, xOffset, yOffset;
		if (document.all){
			xMax = screen.width;
			yMax = screen.height;
		}
		else{
			if (document.layers){
			xMax = window.outerWidth;
			yMax = window.outerHeight;
			}
			else{
			xMax = 800;
			yMax = 600;
			}
		}
		xOffset = (xMax - 800) / 2;
		yOffset = (yMax - 600) / 2;
		window.open(tURL,'_blank','width='+nWidth+',height='+nHeight+',screenX='+xOffset+',screenY='+yOffset+',top='+yOffset+',left='+xOffset);
	} 
//-->
  </SCRIPT>

