<!--startclickprintinclude-->

<h1>Uncle Sam vs. Miss Cleo</h1>

<h3>Regulators take on psychic hot line for deceptive tactics</h3>
<!-- date -->
<script language="JavaScript" type="text/javascript">
<!--
